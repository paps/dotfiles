! caps lock is control
remove lock = Caps_Lock
keycode 66 = Control_L
add control = Control_L

! win + hjkl = arrows
keycode 133 = Mode_switch
keysym h = h H Left
keysym j = j J Down
keysym k = k K Up
keysym l = l L Right

! <> is shift (FR keyboard layout)
keycode 94 = Shift_L

! print screen is insert (useful for Dell XPS 13 keyboard layout)
!keysym Print = Insert

! print screen is compose key (useful for ThinkPad Carbon X1 keyboard layout)
keycode 107 = Multi_key

! eject is delete (apple keyboard...)
keycode 169 = Delete
